{
    "name": "landsoul",
    "version": "0.3.0",
    "type": "module",
    "description": "drop in css library landsoul",
    "main": "./dist/landsoul.css",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "node dev-server.ts",
        "build": "rm -rf ./dist && sass src:dist --no-source-map --silence-deprecation=if-function && postcss ./dist/*.css -d ./dist",
        "format": "prettier --write ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/YieldRay/landsoul.git"
    },
    "keywords": [
        "classless",
        "css"
    ],
    "author": "YieldRay",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/YieldRay/landsoul/issues"
    },
    "homepage": "https://github.com/YieldRay/landsoul#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "devDependencies": {
        "@types/node": "^24.10.4",
        "autoprefixer": "^10.4.23",
        "cssnano": "^7.1.2",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "postcss-preset-env": "^10.6.0",
        "prettier": "^3.7.4",
        "sass": "^1.97.1"
    }
}
